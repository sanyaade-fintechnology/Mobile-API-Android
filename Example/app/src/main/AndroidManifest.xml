    <application android:allowBackup="true"
        android:label="@string/app_name" >

        <activity
            android:name="com.payleven.payment.api.example.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.payleven.payment.api.example.ResultActivity"
            android:label="@string/app_name" >
        </activity>

    </application>

</manifest>
